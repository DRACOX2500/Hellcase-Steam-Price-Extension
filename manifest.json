{
  "manifest_version": 3,
  "name": "Hellcase Steam Price",
  "description": "Get item price from Steam marketplace",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": [
        "https://hellcase.com/*"
      ]
    }
  ]
}